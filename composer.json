{
    "name": "cyberelysium/ce-crud",
    "description": "Generate files for Crud process in laravel",
    "keywords": [
        "cyberelysium",
        "ce-crud",
        "laravel",
        "crud",
        "rest",
        "generator",
        "rest-api",
        "laravel-package",
        "crud-generator",
        "laravel-crud-generator",
        "composer-package",
        "laravel-crud",
        "laravel8",
        "laravel9",
        "laravel10",
        "laravel11"
    ],
    "homepage": "https://github.com/cyberelysium/ce-crud",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Sameera Pathmakumara",
            "email": "klsameerapathmakumara@gmail.com",
            "role": "Developer",
            "homepage": "https://cyberelysium.com/"
        }
    ],
    "require": {
        "php": ">=8.0.0"
    },
    "autoload": {
        "psr-4": {
            "Cyberelysium\\CeCrud\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cyberelysium\\CeCrud\\CeCrudServiceProvider"
            ]
        }
    }
}